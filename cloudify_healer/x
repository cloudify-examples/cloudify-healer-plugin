from cloudify_rest_client import CloudifyClient

client = CloudifyClient("127.0.0.1",username="admin",password="admin",tenant="default_tenant")

executions = client.executions.list( deployment_id = "p" )

last_install = None

for e in executions:
  last_install = e.id if ( e.workflow_id == "install" and ( e.status == "terminated" or e.status == "completed" )) else last_install

return last_install
